{{!-- Need to pass in data! --}}
<div>
    <ul class="collapsible popout" data-collapsible="collapsible">
        {{#each resources as |resource|}}
            <li>
                {{#if resource.isImage}}
                    {{resource-image resource=resource}}
                {{else if resource.isLink}}
                    {{resource-link resource=resource}}
                {{else if resource.isMap}}
                    {{resource-map resource=resource}}
                {{else}}
                    {{resource-text resource=resource}}
                {{/if}}
            </li>
        {{else}}
            {{!-- No items in the resources list --}}
            <div class="collapsible-header">
                <i class="material-icons">warning</i>No Resources
            </div>
        {{/each}}
    </ul>
</div>
